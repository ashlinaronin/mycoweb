{
    "manifest_version": 2,

    "name": "MycoWeb",
    "description": "This extension makes the mycelium underneath the Internet visible",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": ["sexystyles.css"],
            "js": [
                "gsap/gsap.min.js",
                "tree.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ]
}

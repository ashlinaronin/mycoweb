{
    "manifest_version": 2,

    "name": "Style Web",
    "description": "This extension adds a sexy stylesheet to every page",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": ["sexystyles.css"],
            "js": ["inject-styles.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ]
}
